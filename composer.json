{
    "name": "marcosborges/archetype_api_siler",
    "version" : "0.0.2",
    "description": "Arquetype baseline for API using Siler Framework",
    "authors": [
        {
            "name": "Marcos Borges",
            "email": "contato@marcosborges.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "leocavalcante/siler": "dev-master",
        "webonyx/graphql-php": "^0.13.8",
        "monolog/monolog": "^2.0",
        "doctrine/annotations": "^1.10",
        "doctrine/orm": "^2.7"
    },
    "require-dev": {
        "swoole/ide-helper": "^4.4",
        "phpunit/phpunit": "^9.1",
        "phpunit/php-code-coverage": "^8.0"
    },
    "license": "MIT",
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Test\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "doctrine": "doctrine"
    }
}
